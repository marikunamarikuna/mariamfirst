fill(5, 5, 5);
ellipse(200, 200, 375, 375);
fill(102, 0, 255);
triangle(200, 100, 280, 280, 120, 280);
fill(255, 255, 255);
var insert = random(0,5);
var answer = floor(insert);
if (answer < 1) {
    text("YES", 176, 200);
}
else if (answer < 2) {
    text("კი", 176, 200);
}
else if (answer < 3) {
    text("არა", 176, 200);
}
else if (answer < 4) {
    text("ალბათ", 176, 200);
    text("კი", 159, 229); 
}
else {
    text("რათქმაუნდა", 176, 200);
}
